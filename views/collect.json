{
  "$jason": {
    "head": {
      "title": "Collect",
      "actions": {
        "newSpinMessage": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/access/currentSpinMessage.php",
            "method": "post",
            "data": {
              "spinsLeft": "{{$global.spinsLeft}}",
              "nextSpin": "{{$cache.nextSpin}}",
              "collectStatus": "{{$cache.collectStatus}}"
            }
          },
          "success": {
            "type": "$cache.set",
            "options": {
              "currentSpinMessage": "{{$jason}}"
            },
            "success": {
              "type": "$href",
              "options": {
                "url": "https://collectmemes.com/views/collect.json",
                "transition": "push"
              }
            }
          }
        },
        "mainLoad": [{
          "{{#if $global.firstLoadDone == 0}}": {
            "type": "$cache.set",
            "options": {
              "collectStatus": "1",
              "collected": "0"
            },
            "success": {
              "type": "$network.request",
              "options": {
                "url": "https://collectmemes.com/access/getMeme.php",
                "method": "post",
                "data": {
                  "userId": "{{$global.userKey}}",
                  "pass": "933kfjhga7862344bv",
                  "spinsLeft": "{{$global.spinsLeft}}"
                }
              },
              "success": {
                "type": "$cache.set",
                "options": {
                  "current": "{{$jason}}",
                  "liked": "{{$jason.liked}}"
                },
                "success": {
                  "type": "$cache.set",
                  "options": {
                    "spinStatus": "1"
                  },
                  "success": {
                    "type": "$network.request",
                    "options": {
                      "url": "https://collectmemes.com/access/checkTime.php",
                      "method": "post",
                      "data": {
                        "sub": "1"
                      }
                    },
                    "success": {
                      "type": "$cache.set",
                      "options": {
                        "nextSpin": "{{$jason.time}}"
                      },
                      "success": {
                        "type": "$global.set",
                        "options": {
                          "firstLoadDone": "1"
                        },
                        "success": {
                          "trigger": "newSpinMessage"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        {
          "{{#else}}": [{
              "{{#if $cache.collectStatus == 1}}": {
                "type": "$render"
              }
            },
            {
              "{{#else}}": {
                "type": "$network.request",
                "options": {
                  "url": "https://collectmemes.com/access/checkTime.php"
                },
                "success": [{
                  "{{#if $jason.time > $cache.nextSpin}}": {
                    "type": "$cache.set",
                    "options": {
                      "collectStatus": "1"
                    },
                    "success": {
                        "type": "$cache.set",
                        "options": {
                          "current": {
                            "html": "<html>
                            <head>
                            <script src='https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js'></script>
                            <style>
                              .center {
                                width:100%;
                                margin:0;
                                padding:0;
                              }

                              body {
                                background:#111111;margin:0;height:100%;
                              }

                              .num {
                                color:green;
                              }
                            .buttons {
                              width:100%;
                              height:12%;
                              margin-top:6%;
                              color:#fff;
                              padding:0;
                              font-size:6vw;
                              text-align:center;
                            }

                            .spin-again {
                              display:inline-block;float:left;width:48%;height:100%;background:#34495e;border-radius:0px 35px 35px 0px;
                            }

                            .collect-meme {
                              display:inline-block;float:right;width:48%;height:100%;background:#3b9fdf;border-radius:35px 0px 0px 35px;
                            }

                            .centered-text {
                              display: table;
                                height: 100%;
                                width: 100%;
                            }

                            .centered-text span {
                              display: table-cell;
                                vertical-align: middle;
                            }

                            .box {
                            display: flex;
                            width: 100%;
                            height: 8px;
                            margin: 0px 0px 0px 0px;
                            }

                            .box-sm {
                            height: 8px;
                            margin: 0;
                            flex-grow: 1;
                            transition: all .8s ease-in-out;
                            cursor: pointer;
                            }
                            .out {
                            background-color: #57bbfb;
                            }

                            .mid {
                            background-color: #3b9fdf;
                            }

                            .centerb {
                            background-color: #1a7ebe;
                            }
                            .info {
                            width:100%;
                            text-align:center;
                            display:table;
                            margin:20px 0px;
                            padding:0;
                            height:60px;
                            color:#111111;
                            }
                            .section-right {
                            display:table-cell;
                            width:16%;
                            background:#111111;
                            vertical-align:middle;
                            padding:4px;
                            font-size:20px;
                            }

                            .section-middle {
                            display:table-cell;
                            width:68%;
                            background:#0f0f0f;
                            color:white;
                            font-size:20px;
                            vertical-align:middle;
                            }

                            .section-left {
                            display:table-cell;
                            width:16%;
                            background:#111111;
                            color:white;
                            vertical-align:middle;
                            padding:4px;
                            font-size:16px;
                            }
                            .source {
                            font-size:15px;
                            }
                            .img {
                            width:80%;
                            margin:auto;
                            }
                            </style>
                            </head>
                            <body>
                            <div class='info'>
                              <div class='section-left'>
                                <div class='inner'></div>
                              </div>
                              <div class='section-middle'>
                                <div class='title'>Spin to get started!</div>
                              </div>
                              <div class='section-right'>
                                <div class='inner'></div>
                              </div>
                            </div>
                            <div class='img'>
                              <img class='rotate-scale-up center child' src='https://collectmemes.com/img/start.png'>
                            </div>
                            <script>
                            var cw = $('.child').width();
                            $('.child').css({'height':cw+'px'});
                            </script>
                            </body></html>"
                          }
                        },
                        "success": {
                          "type": "$cache.set",
                          "options": {
                            "spinStatus": "1"
                          },
                          "success": {
                            "type": "$global.set",
                            "options": {
                              "spinsLeft": "10"
                            },
                            "success": {
                              "trigger": "newSpinMessage"
                            }
                          }
                        }
                      }
                  }
                },
                {
                  "{{#else}}": {
                    "type": "$network.request",
                    "options": {
                      "url": "https://collectmemes.com/access/checkTime.php"
                    },
                    "success": [
                      {
                        "{{#if $cache.collected == 1}}": {
                          "type": "$util.banner",
                          "options": {
                            "title": "Congrats!",
                            "description": "Meme collected. Check your Vault!"
                          },
                          "success": {
                            "type": "$cache.set",
                            "options": {
                              "collected": "0"
                            },
                            "success": {
                              "type": "$render"
                            }
                          }
                        }
                      },
                      {
                        "{{#else}}": {
                          "type": "$render"
                        }
                      }
                    ]
                  }
                }
                ]
              }
            }
          ]
        }],
        "yeetLoad": [
          {
            "{{#if $global.userKey != '0'}}": {
              "trigger": "mainLoad"
            }
          },
          {
            "{{#else}}": {
              "type": "$href",
              "options": {
                "url": "https://collectmemes.com/views/welcome.json"
              }
            }
          }
        ],
        "$load": [
          {
            "{{#if 'userKey' in $global}}": {
              "trigger": "yeetLoad"
            }
          },
          {
            "{{#else}}": {
              "type": "$global.set",
              "options": {
                "userKey": "0"
              },
              "success": {
                "trigger": "yeetLoad"
              }
            }
          }
        ],
        "onSpin": {
          "type": "$global.set",
          "options": {
            "spinsLeft": "{{$global.spinsLeft - 1}}"
          },
          "success": {
            "type": "$network.request",
            "options": {
              "url": "https://collectmemes.com/access/getMeme.php",
              "method": "post",
              "data": {
                "userId": "{{$global.userKey}}",
                "pass": "933kfjhga7862344bv",
                "spinsLeft": "{{$global.spinsLeft}}"
              }
            },
            "success": {
              "type": "$cache.set",
              "options": {
                "current": "{{$jason}}",
                "liked": "{{$jason.liked}}"
              },
              "success": [
                {
                  "{{#if $global.spinsLeft == 0}}": {
                    "type": "$cache.set",
                    "options": {
                      "spinStatus": "0"
                    },
                    "success": {
                      "type": "$network.request",
                      "options": {
                        "url": "https://collectmemes.com/access/checkTime.php",
                        "method": "post",
                        "data": {
                          "add": "900"
                        }
                      },
                      "success": {
                        "type": "$cache.set",
                        "options": {
                          "nextSpin": "{{$jason.time}}"
                        },
                        "success": {
                          "trigger": "newSpinMessage"
                        }
                      }
                    }
                  }
                },
                {
                  "{{#else}}": {
                    "trigger": "newSpinMessage"
                  }
                }
              ]
            }
          }
        },
        "onCollect": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/access/checkTime.php",
            "method": "post",
            "data": {
              "add": "900"
            }
          },
          "success": {
            "type": "$cache.set",
            "options": {
              "nextSpin": "{{$jason.time}}",
              "collectStatus": "0",
              "spinStatus": "0"
            },
            "success": {
              "type": "$network.request",
              "options": {
                "url": "https://collectmemes.com/access/addToCollection.php",
                "method": "post",
                "data": {
                  "userId": "{{$global.userKey}}",
                  "memeId": "{{$cache.current.id}}",
                  "spinsLeft": "{{$global.spinsLeft}}"
                }
              },
              "success": {
                "type": "$cache.set",
                "options": {
                  "collected": "1"
                },
                "success": {
                  "trigger": "newSpinMessage"
                }
              }
            }
          }
        },
        "likeMeme": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/access/likeMeme.php",
            "method": "post",
            "data": {
              "userId": "{{$global.userKey}}",
              "memeId": "{{$cache.current.id}}"
            }
          },
          "success": {
            "type": "$cache.set",
            "options": {
              "liked": "1"
            },
            "success": {
              "type": "$render"
            }
          }
        },
        "unlikeMeme": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/access/unlikeMeme.php",
            "method": "post",
            "data": {
              "userId": "{{$global.userKey}}",
              "memeId": "{{$cache.current.id}}"
            }
          },
          "success": {
            "type": "$cache.set",
            "options": {
              "liked": "0"
            },
            "success": {
              "type": "$render"
            }
          }
        }
      },
      "templates": {
        "body": {
          "style": {
            "background": {
              "type": "html",
              "text": [
                {
                  "{{#if $global.firstLoadDone == 1}}": "{{$cache.current.html}}"
                },
                {
                  "{{#else}}": "UwU"
                }
              ]
            }
          },
          "header": {
            "style": {
              "background": "#ffffff"
            },
            "title": {
              "type": "image",
              "style": {
                "width": "94",
                "height": "27"
              },
              "url": "https://collectmemes.com/img/logo.png"
            },
            "menu": {
              "image": "https://collectmemes.com/img/menu2.png",
              "href": {
                "url": "https://collectmemes.com/views/profile.json",
                "transition": "push"
              }
            }
          },
          "layers": [
            {
              "type": "image",
              "url": [
                {
                  "{{#if $global.spinsLeft == 10}}": "https://collectmemes.com/img/empty.png"
                },
                {
                  "{{#elseif $cache.liked == 1}}": "https://collectmemes.com/img/heart-full.png"
                },
                {
                  "{{#else}}": "https://collectmemes.com/img/heart-white.png"
                }
              ],
              "style": {
                "width": "40",
                "height": "40",
                "top": "86",
                "left": "86%-8"
              },
              "action": [
                {
                  "{{#if $global.spinsLeft !== 10 && $cache.liked == 0}}": {
                    "trigger": "likeMeme"
                  }
                },
                {
                  "{{#elseif $cache.liked == 1}}": {
                    "trigger": "unlikeMeme"
                  }
                }
              ]
            },
            {
              "type": "button",
              "text": "Spin",
              "style": {
                "width": "47%",
                "height": "8%",
                "align": "center",
                "top": "61%+50",
                "left": "2%",
                "background": [
                  {
                    "{{#if $cache.spinStatus == 1}}": "#34495e"
                  },
                  {
                    "{{#else}}": "#1A232E"
                  }
                ],
                "color": "#ffffff",
                "size": "22",
                "corner_radius": "25"
              },
              "action": [
                {
                  "{{#if $cache.spinStatus == 1}}": {
                    "trigger": "onSpin"
                  }
                },
                {
                  "{{#else}}": {
                    "type": "$util.banner",
                    "options": {
                      "title": "Out of Spins:",
                      "description": "We know it's fun... spin some more after 15 min!"
                    }
                  }
                }
              ]
            },
            {
              "type": "html",
              "text": "{{$cache.currentSpinMessage.html}}",
              "style": {
                "width": "47%",
                "height": "8%",
                "align": "center",
                "top": "68%+55",
                "left": "2%"
              }
            },
            {
              "type": "button",
              "text": "Collect!",
              "style": {
                "width": "47%",
                "height": "8%",
                "align": "center",
                "top": "61%+50",
                "left": "51%",
                "background": [
                  {
                    "{{#if $cache.collectStatus == 1}}": "#319CDE"
                  },
                  {
                    "{{#else}}": "#11364D"
                  }
                ],
                "color": "#ffffff",
                "size": "22",
                "corner_radius": "25"
              },
              "action": [
                {
                  "{{#if $cache.collectStatus == 1}}": [
                    {
                      "{{#if $global.spinsLeft == 10}}": {
                        "type": "$util.banner",
                        "options": {
                          "title": "Whoops...",
                          "description": "Spin for a meme before collecting!"
                        }
                      }
                    },
                    {
                      "{{#else}}": {
                        "trigger": "onCollect"
                      }
                    }
                  ]
                },
                {
                  "{{#else}}": {
                    "type": "$util.banner",
                    "options": {
                      "title": "Collect Expended:",
                      "description": "Make each meme count! Collect again in 15 min."
                    }
                  }
                }
              ]
            },
            {
              "type": "image",
              "url": "https://collectmemes.com/img/vault2.png",
              "style": {
                "width": "64",
                "height": "64",
                "right": "5%",
                "bottom": "3%"
              },
              "action": {
                "type": "$href",
                "options": {
                  "url": "https://collectmemes.com/views/vault.json",
                  "transition": "push"
                }
              }
            }
          ]
        }
      }
    }
  }
}
