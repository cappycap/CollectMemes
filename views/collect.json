{
  "$jason": {
    "head": {
      "title": "Collect",
      "actions": {
        "newSpinMessage": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/access/currentSpinMessage.php",
            "method": "post",
            "data": {
              "spinsLeft": "{{$global.spinsLeft}}",
              "nextSpin": "{{$cache.nextSpin}}",
              "collectStatus": "{{$cache.collectStatus}}"
            }
          },
          "success": {
            "type": "$cache.set",
            "options": {
              "currentSpinMessage": "{{$jason}}"
            },
            "success": {
              "type": "$href",
              "options": {
                "url": "https://collectmemes.com/views/collect.json",
                "transition": "push"
              }
            }
          }
        },
        "startTimer": {
          "type": "$render",
          "success": {
            "type": "$timer.start",
            "options": {
              "interval": "{{$cache.nextSpin - $jason.time}}",
              "name": "timer1",
              "repeats": "false",
              "action": {
                "type": "$cache.set",
                "options": {
                  "collectStatus": "1"
                },
                "success": {
                  "type": "$network.request",
                  "options": {
                    "url": "https://collectmemes.com/access/getMeme.php",
                    "method": "post",
                    "data": {
                      "userId": "{{$global.userKey}}",
                      "pass": "933kfjhga7862344bv",
                      "spinsLeft": "{{$global.spinsLeft}}"
                    }
                  },
                  "success": {
                    "type": "$cache.set",
                    "options": {
                      "current": "{{$jason}}",
                      "liked": "0"
                    },
                    "success": {
                      "type": "$cache.set",
                      "options": {
                        "spinStatus": "1"
                      },
                      "success": {
                        "type": "$timer.stop",
                        "options": {
                          "name": "timer1"
                        },
                        "success": {
                          "type": "$global.set",
                          "options": {
                            "spinsLeft": "10"
                          },
                          "success": {
                            "trigger": "newSpinMessage"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "mainLoad": [{
          "{{#if $global.firstLoadDone == 0}}": {
            "type": "$cache.set",
            "options": {
              "collectStatus": "1",
              "collected": "0"
            },
            "success": {
              "type": "$network.request",
              "options": {
                "url": "https://collectmemes.com/access/getMeme.php",
                "method": "post",
                "data": {
                  "userId": "{{$global.userKey}}",
                  "pass": "933kfjhga7862344bv",
                  "spinsLeft": "{{$global.spinsLeft}}"
                }
              },
              "success": {
                "type": "$cache.set",
                "options": {
                  "current": "{{$jason}}",
                  "liked": "0"
                },
                "success": {
                  "type": "$cache.set",
                  "options": {
                    "spinStatus": "1"
                  },
                  "success": {
                    "type": "$network.request",
                    "options": {
                      "url": "https://collectmemes.com/access/checkTime.php",
                      "method": "post",
                      "data": {
                        "sub": "1"
                      }
                    },
                    "success": {
                      "type": "$cache.set",
                      "options": {
                        "nextSpin": "{{$jason.time}}"
                      },
                      "success": {
                        "type": "$global.set",
                        "options": {
                          "firstLoadDone": "1"
                        },
                        "success": {
                          "trigger": "newSpinMessage"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        {
          "{{#else}}": [{
              "{{#if $cache.collectStatus == 1}}": {
                "type": "$render"
              }
            },
            {
              "{{#else}}": {
                "type": "$network.request",
                "options": {
                  "url": "https://collectmemes.com/access/checkTime.php"
                },
                "success": [{
                  "{{#if $jason.time > $cache.nextSpin}}": {
                    "type": "$cache.set",
                    "options": {
                      "collectStatus": "1"
                    },
                    "success": {
                      "type": "$network.request",
                      "options": {
                        "url": "https://collectmemes.com/access/getMeme.php",
                        "method": "post",
                        "data": {
                          "userId": "{{$global.userKey}}",
                          "pass": "933kfjhga7862344bv",
                          "spinsLeft": "{{$global.spinsLeft}}"
                        }
                      },
                      "success": {
                        "type": "$cache.set",
                        "options": {
                          "current": "{{$jason}}",
                          "liked": "0"
                        },
                        "success": {
                          "type": "$cache.set",
                          "options": {
                            "spinStatus": "1"
                          },
                          "success": {
                            "type": "$global.set",
                            "options": {
                              "spinsLeft": "10"
                            },
                            "success": {
                              "trigger": "newSpinMessage"
                            }
                          }
                        }
                      }
                    }
                  }
                },
                {
                  "{{#else}}": {
                    "type": "$network.request",
                    "options": {
                      "url": "https://collectmemes.com/access/checkTime.php"
                    },
                    "success": [
                      {
                        "{{#if $cache.collected == 1}}": {
                          "type": "$util.banner",
                          "options": {
                            "title": "Congrats!",
                            "description": "Meme collected. Check your Vault!"
                          },
                          "success": {
                            "type": "$cache.set",
                            "options": {
                              "collected": "0"
                            },
                            "success": {
                              "trigger": "startTimer"
                            }
                          }
                        }
                      },
                      {
                        "{{#else}}": {
                          "trigger": "startTimer"
                        }
                      }
                    ]
                  }
                }
                ]
              }
            }
          ]
        }],
        "$load": [
          {
            "{{#if 'userKey' in $global}}": {
              "trigger": "mainLoad"
            }
          },
          {
            "{{#else}}": {
              "type": "$href",
              "options": {
                "url": "https://collectmemes.com/views/welcome.json"
              }
            }
          }
        ],
        "onSpin": {
          "type": "$global.set",
          "options": {
            "spinsLeft": "{{$global.spinsLeft - 1}}"
          },
          "success": {
            "type": "$network.request",
            "options": {
              "url": "https://collectmemes.com/access/getMeme.php",
              "method": "post",
              "data": {
                "userId": "{{$global.userKey}}",
                "pass": "933kfjhga7862344bv",
                "spinsLeft": "{{$global.spinsLeft}}"
              }
            },
            "success": {
              "type": "$cache.set",
              "options": {
                "current": "{{$jason}}",
                "liked": "0"
              },
              "success": [
                {
                  "{{#if $global.spinsLeft == 0}}": {
                    "type": "$cache.set",
                    "options": {
                      "spinStatus": "0"
                    },
                    "success": {
                      "type": "$network.request",
                      "options": {
                        "url": "https://collectmemes.com/access/checkTime.php",
                        "method": "post",
                        "data": {
                          "add": "900"
                        }
                      },
                      "success": {
                        "type": "$cache.set",
                        "options": {
                          "nextSpin": "{{$jason.time}}"
                        },
                        "success": {
                          "trigger": "newSpinMessage"
                        }
                      }
                    }
                  }
                },
                {
                  "{{#else}}": {
                    "trigger": "newSpinMessage"
                  }
                }
              ]
            }
          }
        },
        "onCollect": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/access/checkTime.php",
            "method": "post",
            "data": {
              "add": "900"
            }
          },
          "success": {
            "type": "$cache.set",
            "options": {
              "nextSpin": "{{$jason.time}}",
              "collectStatus": "0",
              "spinStatus": "0"
            },
            "success": {
              "type": "$network.request",
              "options": {
                "url": "https://collectmemes.com/access/addToCollection.php",
                "method": "post",
                "data": {
                  "userId": "{{$global.userKey}}",
                  "memeId": "{{$cache.current.id}}"
                }
              },
              "success": {
                "type": "$cache.set",
                "options": {
                  "collected": "1"
                },
                "success": {
                  "trigger": "newSpinMessage"
                }
              }
            }
          }
        },
        "likeMeme": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/access/likeMeme.php",
            "method": "post",
            "data": {
              "userId": "{{$global.userKey}}",
              "memeId": "{{$cache.current.id}}"
            }
          },
          "success": {
            "type": "$cache.set",
            "options": {
              "liked": "1"
            },
            "success": {
              "type": "$render"
            }
          }
        },
        "unlikeMeme": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/access/unlikeMeme.php",
            "method": "post",
            "data": {
              "userId": "{{$global.userKey}}",
              "memeId": "{{$cache.current.id}}"
            }
          },
          "success": {
            "type": "$cache.set",
            "options": {
              "liked": "0"
            },
            "success": {
              "type": "$render"
            }
          }
        }
      },
      "templates": {
        "body": {
          "style": {
            "background": {
              "type": "html",
              "text": [
                {
                  "{{#if $global.firstLoadDone == 1}}": "{{$cache.current.html}}"
                },
                {
                  "{{#else}}": "UwU"
                }
              ]
            }
          },
          "header": {
            "style": {
              "background": "#ffffff"
            },
            "title": {
              "type": "image",
              "style": {
                "width": "94",
                "height": "27"
              },
              "url": "https://collectmemes.com/img/logo.png"
            },
            "menu": {
              "image": "https://collectmemes.com/img/menu.png",
              "href": {
                "url": "https://collectmemes.com/views/collect.json",
                "transition": "push"
              }
            }
          },
          "layers": [
            {
              "type": "image",
              "url": [
                {
                  "{{#if $global.spinsLeft == 10}}": "https://collectmemes.com/img/empty.png"
                },
                {
                  "{{#elseif $cache.liked == 1}}": "https://collectmemes.com/img/heart-full.png"
                },
                {
                  "{{#else}}": "https://collectmemes.com/img/heart-white.png"
                }
              ],
              "style": {
                "width": "40",
                "height": "40",
                "top": "86",
                "left": "86%-8"
              },
              "action": [
                {
                  "{{#if $global.spinsLeft !== 10 && $cache.liked == 0}}": {
                    "trigger": "likeMeme"
                  }
                },
                {
                  "{{#elseif $cache.liked == 1}}": {
                    "trigger": "unlikeMeme"
                  }
                }
              ]
            },
            {
              "type": "label",
              "text": "Spin",
              "style": {
                "width": "47%",
                "height": "8%",
                "align": "center",
                "top": "61%+50",
                "left": "2%",
                "background": [
                  {
                    "{{#if $cache.spinStatus == 1}}": "#34495e"
                  },
                  {
                    "{{#else}}": "#1A232E"
                  }
                ],
                "color": "#ffffff",
                "size": "22",
                "corner_radius": "25"
              },
              "action": [
                {
                  "{{#if $cache.spinStatus == 1}}": {
                    "trigger": "onSpin"
                  }
                },
                {
                  "{{#else}}": {
                    "type": "$util.banner",
                    "options": {
                      "title": "Out of Spins",
                      "description": "We know it's fun... spin some more after 15 minutes!"
                    }
                  }
                }
              ]
            },
            {
              "type": "html",
              "text": "{{$cache.currentSpinMessage.html}}",
              "style": {
                "width": "47%",
                "height": "8%",
                "align": "center",
                "top": "68%+55",
                "left": "2%"
              }
            },
            {
              "type": "label",
              "text": "Collect!",
              "style": {
                "width": "47%",
                "height": "8%",
                "align": "center",
                "top": "61%+50",
                "left": "51%",
                "background": [
                  {
                    "{{#if $cache.collectStatus == 1}}": "#319CDE"
                  },
                  {
                    "{{#else}}": "#11364D"
                  }
                ],
                "color": "#ffffff",
                "size": "22",
                "corner_radius": "25"
              },
              "action": [
                {
                  "{{#if $cache.collectStatus == 1}}": [
                    {
                      "{{#if $global.spinsLeft == 10}}": {
                        "type": "$util.banner",
                        "options": {
                          "title": "Whoops...",
                          "description": "Spin for a meme before collecting!"
                        }
                      }
                    },
                    {
                      "{{#else}}": {
                        "trigger": "onCollect"
                      }
                    }
                  ]
                },
                {
                  "{{#else}}": {
                    "type": "$util.banner",
                    "options": {
                      "title": "Whoa there!",
                      "description": "You can only collect every 15 minutes! Make each meme count."
                    }
                  }
                }
              ]
            },
            {
              "type": "image",
              "url": "https://collectmemes.com/img/vault2.png",
              "style": {
                "width": "64",
                "height": "64",
                "right": "5%",
                "bottom": "3%"
              }
            }
          ]
        }
      }
    }
  }
}
