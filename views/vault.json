{
  "$jason": {
    "head": {
      "data": {
        "load-phrases": [
          { "phrase": "?" }
        ]
      },
      "title": "Vault",
      "actions": {
        "$load": [
          {
            "{{#if typeOf $cache.cur == 'undefined'}}": {
              "type": "$cache.set",
              "options": {
                "cur": "1"
                "sort": "rank",
                "sortDir": "1"
              },
              "success": {
                "trigger": "fetchImages"
              }
            }
          },
          {
            "{{#else}}": {
              "trigger": "fetchImages"
            }
          }
        ],
        "fetchImages": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/access/getVaultCollection.php",
            "method": "post",
            "data": {
              "userId": "{{$global.userKey}}",
              "cur": "{{$cache.cur}}",
              "sort": "{{$cache.sort}}",
              "sortDir": "{{$cache.sortDir}}"
            }
          },
          "success": {
            "type": "$cache.set",
            "options": {
              "components": "{{$jason.components}}",
              "size": "{{$jason.size}}"
            }
          }
        }
      },
      "templates": {
        "body": {
          "background": "#111111",
          "style": {
            "border": "none"
          },
          "header": {
            "style": {
              "background": "#ffffff"
            },
            "title": {
              "type": "image",
              "style": {
                "width": "94",
                "height": "27"
              },
              "url": "https://collectmemes.com/img/logo.png"
            },
            "menu": {
              "image": "https://collectmemes.com/img/menu.png",
              "href": {
                "url": "https://collectmemes.com/views/vault.json",
                "transition": "push"
              }
            }
          },
          "layers": [
            {
              "type": "image",
              "url": "https://collectmemes.com/img/heart-white.png",
              "style": {
                "width": "40",
                "height": "40",
                "top": "70",
                "left": "86%+2"
              }
            },
            {
              "type": "label",
              "text": "likes",
              "style": {
                "width": "40",
                "align": "center",
                "height": "40",
                "left": "86%+2",
                "top": "70",
                "size": "10",
                "color": "#ffffff"
              },
              "href": {
                "url": "https://collectmemes.com/views/likes.json",
                "transition": "push"
              }
            },
            {
              "type": "label",
              "text": ".",
              "style": {
                "width": "40",
                "height": "40",
                "left": "2%",
                "top": "123",
                "background": "#dedede",
                "color": "#dedede"
              }
            },
            {
              "type": "image",
              "url": [
                {
                  "{{#if $cache.sortDir == 1}}": "https://collectmemes.com/img/sort-up.png"
                },
                {
                  "{{#else}}": "https://collectmemes.com/img/sort-down.png"
                }
              ],
              "style": {
                "width": "30",
                "height": "30",
                "left": "2%+5",
                "top": "128"
              }
            },
            {
              "type": "button",
              "text": "Rank",
              "style": {
                "width": "30%-41",
                "height": "40",
                "left": "2%+41",
                "top": "123",
                "align": "center",
                "background": "#dedede",
                "color": "#111111",
                "size": "16"
              }
            },
            {
              "type": "button",
              "text": "Recent",
              "style": {
                "width": "30%",
                "height": "40",
                "left": "35%",
                "top": "123",
                "align": "center",
                "background": "#A3A3A3",
                "color": "#111111",
                "size": "16"
              }
            },
            {
              "type": "label",
              "text": ".",
              "style": {
                "width": "30",
                "height": "30",
                "left": "2%",
                "top": "82%+3",
                "background": "#dedede",
                "color": "#dedede",
                "corner_radius": "15"
              }
            },
            {
              "type": "image",
              "url": "https://collectmemes.com/img/left.png",
              "style": {
                "width": [
                  {
                    "{{#if $cache.cur >= 10}}": "20"
                  },
                  {
                    "{{#else}}": "0"
                  }
                ],
                "height": "20",
                "left": "2%+5",
                "top": "82%+8"
              },
              "action": {
                "type": "$cache.set",
                "options": {
                  "cur": "{{$cache.cur - 9}}"
                },
                "success": {
                  "type": "$reload"
                }
              }
            },
            {
              "type": "label",
              "text": ".",
              "style": {
                "width": "30",
                "height": "30",
                "left": "98%-30",
                "top": "82%+3",
                "background": "#dedede",
                "color": "#dedede",
                "corner_radius": "15"
              }
            },
            {
              "type": "image",
              "url": "https://collectmemes.com/img/right.png",
              "style": {
                "width": [
                  {
                    "{{#if $cache.cur + 9 <= $cache.size}}": "20"
                  },
                  {
                    "{{#else}}": "0"
                  }
                ],
                "height": "20",
                "left": "98%-25",
                "top": "82%+8"
              },
              "action": {
                "type": "$cache.set",
                "options": {
                  "cur": "{{$cache.cur + 9}}"
                },
                "success": {
                  "type": "$reload"
                }
              }
            }
          ],
          "sections": [{
            "items": [
              {
                "type": "html",
                "text": "<html>
                <head>
                  <style>
                    body {
                      background:#111111;
                      margin:0;
                    }
                    span {
                      font-size:40px;
                      color:white;
                    }
                    .title {
                      padding:10px;
                      margin-bottom:60px;
                    }
                    .box {
                      display: flex;
                      width: 100%;
                      height: 8px;
                      margin: 0px 0px 0px 0px;
                    }

                    .box-sm {
                      height: 8px;
                      margin: 0;
                      flex-grow: 1;
                      transition: all .8s ease-in-out;
                      cursor: pointer;
                    }
                    .update {
                      background-color: #57bbfb;
                    }

                    .orange {
                      background-color: #3b9fdf;
                    }

                    .yellow {
                      background-color: #1a7ebe;
                    }

                    .green {
                      background-color:#1a7ebe;
                    }

                    .blue {
                      background-color: #3b9fdf;
                    }

                    .purple {
                      background-color: #57bbfb;
                    }

                    .stats {
                        position: absolute;
                        color: #c3c3c3;
                        top: 26px;
                        left: 115px;
                        font-size: 16px;
                        font-style: italic;
                    }
                  </style>
                </head>
                <body>
                  <div class='title'>
                    <span>Vault</span>

                  </div>
                  <div class='stats'>
                    3 memes - avg rank 164
                  </div>
                  <div class='box'>
                    <div class='box-sm update'></div>
                    <div class='box-sm orange'></div>
                    <div class='box-sm yellow'></div>
                    <div class='box-sm green'></div>
                    <div class='box-sm blue'></div>
                    <div class='box-sm update'></div>
                  </div>
                </body>
                </html>",
                "style": {
                  "width": "100%",
                  "height": "133"
                }
              },
              {
                "type": "vertical",
                "style": {
                  "padding": "0",
                  "spacing": "0"
                },
                "components": [
                  {
                    "{{#if $cache.components == 'undefined'}}": [
                      {
                        "type": "label",
                        "text": "You don't have any memes yet. Get collecting!",
                        "style": {
                          "align": "center"
                        }
                      }
                    ]
                  },
                  {
                    "{{#else}}": "{{$cache.components}}"
                  }
                ]
              },
              {
                "type": "html",
                "text": "<html>
                <head>
                  <script src='https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js'></script>
                  <style>
                    body {
                      background:#fff;
                      margin:0;
                    }
                    .box {
                      display: flex;
                      width: 100%;
                      height: 8px;
                      margin: 0px 0px 0px 0px;
                    }

                    .box-sm {
                      height: 8px;
                      margin: 0;
                      flex-grow: 1;
                      transition: all .8s ease-in-out;
                      cursor: pointer;
                    }
                    .update {
                      background-color: #57bbfb;
                    }

                    .orange {
                      background-color: #3b9fdf;
                    }

                    .yellow {
                      background-color: #1a7ebe;
                    }

                    .green {
                      background-color:#1a7ebe;
                    }

                    .blue {
                      background-color: #3b9fdf;
                    }

                    .purple {
                      background-color: #57bbfb;
                    }
                  </style>
                </head>
                <body>
                  <div class='box'>
                    <div class='box-sm update'></div>
                    <div class='box-sm orange'></div>
                    <div class='box-sm yellow'></div>
                    <div class='box-sm green'></div>
                    <div class='box-sm blue'></div>
                    <div class='box-sm update'></div>
                  </div>
                </body>
                </html>",
                "style": {
                  "width": "100%",
                  "height": "8"
                }
              }
            ]
          }],
          "footer": {
            "tabs": {
              "style": {
                "background": "rgba(255,255,255,1)",
                "color": "#000000"
              },
              "items": [
                {
                  "image": "https://collectmemes.com/img/friends.png",
                  "style": {
                    "height": "21"
                  },
                  "url": "https://collectmemes.com/views/friends.php"
                },
                {
                  "image": "https://collectmemes.com/img/collect.png",
                  "style": {
                    "height": "21"
                  },
                  "url": "https://collectmemes.com/views/collect.php"
                },
                {
                  "image": "https://collectmemes.com/img/vault.png",
                  "style": {
                    "height": "21"
                  },
                  "url": "https://collectmemes.com/views/vault.php"
                }
              ]
            }
          }
        }
      }
    }
  }
}
