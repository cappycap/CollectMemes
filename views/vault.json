{
  "$jason": {
    "head": {
      "data": {
        "load-phrases": [
          { "phrase": "?" }
        ]
      },
      "title": "Vault",
      "actions": {
        "mainLoad": [
          {
            "{{#if 'cur' in $cache}}": {
              "trigger": "fetchImages"
            }
          },
          {
            "{{#else}}": {
              "type": "$cache.set",
              "options": {
                "cur": "1",
                "sort": "rank",
                "sortDir": "1"
              },
              "success": {
                "trigger": "fetchImages"
              }
            }
          }
        ],
        "$load": [
          {
            "{{#if 'userKey' in $global}}": {
              "trigger": "mainLoad"
            }
          },
          {
            "{{#else}}": {
              "type": "$href",
              "options": {
                "url": "https://collectmemes.com/views/welcome.json"
              }
            }
          }
        ],
        "fetchImages": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/access/getVaultCollection.php",
            "method": "post",
            "data": {
              "userId": "{{$global.userKey}}",
              "cur": "{{$cache.cur}}",
              "sort": "{{$cache.sort}}",
              "sortDir": "{{$cache.sortDir}}"
            }
          },
          "success": {
            "type": "$cache.set",
            "options": {
              "components": "{{$jason.components}}",
              "size": "{{$jason.size}}",
              "avgRank": "{{$jason.avgRank}}",
              "curAdd": "{{$jason.curAdd}}"
            },
            "success": {
              "type": "$render"
            }
          }
        },
        "onRank": {
          "type": "$cache.set",
          "options": {
            "sort": "rank"
          },
          "success": {
            "type": "$reload"
          }
        },
        "onRecent": {
          "type": "$cache.set",
          "options": {
            "sort": "dateAdded"
          },
          "success": {
            "type": "$reload"
          }
        },
        "onSort": {
          "type": "$cache.set",
          "options": {
            "sortDir": [
              {
                "{{#if $cache.sortDir == 1}}": "0"
              },
              {
                "{{#else}}": "1"
              }
            ]
          },
          "success": {
            "type": "$reload"
          }
        }
      },
      "templates": {
        "body": {
          "background": "#111111",
          "style": {
            "border": "none"
          },
          "header": {
            "style": {
              "background": "#ffffff"
            },
            "title": {
              "type": "image",
              "style": {
                "width": "94",
                "height": "27"
              },
              "url": "https://collectmemes.com/img/logo.png"
            }
          },
          "layers": [
            {
              "type": "image",
              "url": "https://collectmemes.com/img/back.png",
              "style": {
                "align": "left",
                "left": "2%",
                "width": "7%",
                "top": "2%"
              },
              "action": {
                "type": "$href",
                "options": {
                  "url": "https://collectmemes.com/views/welcome.json",
                  "transition": "push"
                }
              }
            },
            {
              "type": "image",
              "url": "https://collectmemes.com/img/heart-white.png",
              "style": {
                "width": "40",
                "height": "40",
                "top": "70",
                "left": "86%+2"
              }
            },
            {
              "type": "label",
              "text": "likes",
              "style": {
                "width": "40",
                "align": "center",
                "height": "40",
                "left": "86%+2",
                "top": "70",
                "size": "10",
                "color": "#ffffff"
              },
              "href": {
                "url": "https://collectmemes.com/views/likes.json",
                "transition": "push"
              }
            },
            {
              "type": "label",
              "text": ".",
              "style": {
                "width": [
                  {
                    "{{#if $cache.components == 'undefined'}}": "0"
                  },
                  {
                    "{{#else}}": "40"
                  }
                ],
                "height": "40",
                "left": [
                  {
                    "{{#if $cache.sort == 'rank'}}": "2%"
                  },
                  {
                    "{{#else}}": "51%"
                  }
                ],
                "top": "123",
                "background": "#dedede",
                "color": "#dedede"
              }
            },
            {
              "type": "image",
              "url": [
                {
                  "{{#if $cache.sortDir == 1}}": "https://collectmemes.com/img/sort-up.png"
                },
                {
                  "{{#else}}": "https://collectmemes.com/img/sort-down.png"
                }
              ],
              "style": {
                "width": [
                  {
                    "{{#if $cache.components == 'undefined'}}": "0"
                  },
                  {
                    "{{#else}}": "30"
                  }
                ],
                "height": "30",
                "left": [
                  {
                    "{{#if $cache.sort == 'rank'}}": "2%+5"
                  },
                  {
                    "{{#else}}": "51%+5"
                  }
                ],
                "top": "128"
              },
              "action": {
                "trigger": "onSort"
              }
            },
            {
              "type": "button",
              "text": "Rank",
              "style": {
                "width": [
                  {
                    "{{#if $cache.components == 'undefined'}}": "0"
                  },
                  {
                    "{{#elseif $cache.sort == 'rank'}}": "47%-41"
                  },
                  {
                    "{{#else}}": "47%"
                  }
                ],
                "height": "40",
                "left": [
                  {
                    "{{#if $cache.sort == 'rank'}}": "2%+41"
                  },
                  {
                    "{{#else}}": "2%"
                  }
                  ],
                "top": "123",
                "align": "center",
                "background": [
                  {
                    "{{#if $cache.sort == 'rank'}}": "#dedede"
                  },
                  {
                    "{{#else}}": "#A3A3A3"
                  }
                ],
                "color": "#111111",
                "size": "16"
              },
              "action": [
                {
                  "{{#if $cache.sort == 'dateAdded'}}": {
                    "trigger": "onRank"
                  }
                }
              ]
            },
            {
              "type": "button",
              "text": "Recent",
              "style": {
                "width": [
                  {
                    "{{#if $cache.components == 'undefined'}}": "0"
                  },
                  {
                    "{{#elseif $cache.sort == 'dateAdded'}}": "47%-41"
                  },
                  {
                    "{{#else}}": "47%"
                  }
                ],
                "height": "40",
                "left": [
                  {
                    "{{#if $cache.sort == 'dateAdded'}}": "51%+41"
                  },
                  {
                    "{{#else}}": "51%"
                  }
                ],
                "top": "123",
                "align": "center",
                "background": [
                  {
                    "{{#if $cache.sort == 'dateAdded'}}": "#dedede"
                  },
                  {
                    "{{#else}}": "#A3A3A3"
                  }
                ],
                "color": "#111111",
                "size": "16"
              },
              "action": [
                {
                  "{{#if $cache.sort == 'rank'}}": {
                    "trigger": "onRecent"
                  }
                }
              ]
            },
            {
              "type": "label",
              "text": ".",
              "style": {
                "width": [
                  {
                    "{{#if $cache.cur >= 10}}": "30"
                  },
                  {
                    "{{#else}}": "0"
                  }
                ],
                "height": "30",
                "left": "2%",
                "top": "82%+3",
                "background": "#dedede",
                "color": "#dedede",
                "corner_radius": "15"
              }
            },
            {
              "type": "image",
              "url": "https://collectmemes.com/img/left.png",
              "style": {
                "width": [
                  {
                    "{{#if $cache.cur >= 10}}": "20"
                  },
                  {
                    "{{#else}}": "0"
                  }
                ],
                "height": "20",
                "left": "2%+5",
                "top": "82%+8"
              },
              "action": {
                "type": "$network.request",
                "options": {
                  "url": "https://collectmemes.com/access/math.php",
                  "method": "post",
                  "data": {
                    "x": "{{$cache.cur}}",
                    "y": "9",
                    "sub": "true"
                  }
                },
                "success": {
                  "type": "$cache.set",
                  "options": {
                    "cur": "{{$jason.ret}}"
                  },
                  "success": {
                    "type": "$reload"
                  }
                }
              }
            },
            {
              "type": "label",
              "text": ".",
              "style": {
                "width": [
                  {
                    "{{#if $cache.curAdd <= $cache.size}}": "30"
                  },
                  {
                    "{{#else}}": "0"
                  }
                ],
                "height": "30",
                "left": "98%-30",
                "top": "82%+3",
                "background": "#dedede",
                "color": "#dedede",
                "corner_radius": "15"
              }
            },
            {
              "type": "image",
              "url": "https://collectmemes.com/img/right.png",
              "style": {
                "width": [
                  {
                    "{{#if $cache.curAdd <= $cache.size}}": "20"
                  },
                  {
                    "{{#else}}": "0"
                  }
                ],
                "height": "20",
                "left": "98%-25",
                "top": "82%+8"
              },
              "action": {
                "type": "$network.request",
                "options": {
                  "url": "https://collectmemes.com/access/math.php",
                  "method": "post",
                  "data": {
                    "x": "{{$cache.cur}}",
                    "y": "9",
                    "add": "true"
                  }
                },
                "success": {
                  "type": "$cache.set",
                  "options": {
                    "cur": "{{$jason.ret}}"
                  },
                  "success": {
                    "type": "$reload"
                  }
                }
              }
            }
          ],
          "sections": [{
            "items": [
              {
                "{{#if $cache.components == 'undefined'}}": [
                  {
                    "type": "label",
                    "text": "No memes collected yet!",
                    "style": {
                      "padding": "20",
                      "align": "center",
                      "color": "#ffffff",
                      "size": "20"
                    }
                  }
                ]
              },
              {
                "{{#else}}": [
                  {
                    "type": "html",
                    "text": "<html>
                    <head>
                      <style>
                        body {
                          background:#111111;
                          margin:0;
                        }
                        span {
                          font-size:40px;
                          color:white;
                        }
                        .title {
                          padding:10px;
                          margin-bottom:60px;
                        }
                        .box {
                          display: flex;
                          width: 100%;
                          height: 8px;
                          margin: 0px 0px 0px 0px;
                        }

                        .box-sm {
                          height: 8px;
                          margin: 0;
                          flex-grow: 1;
                          transition: all .8s ease-in-out;
                          cursor: pointer;
                        }
                        .i {
                          background-color:#d2dae2;
                        }

                        .j {
                          background-color:#CAD1D9;
                        }

                        .w {
                          background-color:#BEC5CC;
                        }

                        .stats {
                            position: absolute;
                            color: #c3c3c3;
                            top: 26px;
                            left: 115px;
                            font-size: 16px;
                            font-style: italic;
                        }
                      </style>
                    </head>
                    <body>
                      <div class='title'>
                        <span>Vault</span>

                      </div>
                      <div class='stats'>
                        {{$cache.size}} memes - avg rank {{$cache.avgRank}}
                      </div>
                    </body>
                    </html>",
                    "style": {
                      "width": "100%",
                      "height": "133"
                    }
                  },
                  {
                    "type": "vertical",
                    "style": {
                      "padding": "0",
                      "spacing": "0"
                    },
                    "components": [
                      {
                        "type": "horizontal",
                        "style": {
                    			"padding": "0",
                    			"spacing": "0"
                    		},
                        "components": [
                          {
                            "type": "html",
                            "text": "{{$cache.components[0].components[0].text}}",
                            "style": {
                              "height": "130"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[0].components[0]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/viewVaultMeme.json",
                                    "memeId": "{{$cache.components[0].components[0].memeId}}"
                                  }
                                }
                              }
                            ]
                          },
                          {
                            "type": "html",
                            "text": "{{$cache.components[0].components[1].text}}",
                            "style": {
                              "height": "130"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[0].components[1]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/viewVaultMeme.json",
                                    "memeId": "{{$cache.components[0].components[1].memeId}}"
                                  }
                                }
                              }
                            ]
                          },
                          {
                            "type": "html",
                            "text": "{{$cache.components[0].components[2].text}}",
                            "style": {
                              "height": "130"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[0].components[2]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/viewVaultMeme.json",
                                    "memeId": "{{$cache.components[0].components[2].memeId}}"
                                  }
                                }
                              }
                            ]
                          }
                        ]
                      },
                      {
                        "type": "horizontal",
                        "style": {
                    			"padding": "0",
                    			"spacing": "0"
                    		},
                        "components": [
                          {
                            "type": "html",
                            "text": "{{$cache.components[1].components[0].text}}",
                            "style": {
                              "height": "130"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[1].components[0]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/viewVaultMeme.json",
                                    "memeId": "{{$cache.components[1].components[0].memeId}}"
                                  }
                                }
                              }
                            ]
                          },
                          {
                            "type": "html",
                            "text": "{{$cache.components[1].components[1].text}}",
                            "style": {
                              "height": "130"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[1].components[1]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/viewVaultMeme.json",
                                    "memeId": "{{$cache.components[1].components[1].memeId}}"
                                  }
                                }
                              }
                            ]
                          },
                          {
                            "type": "html",
                            "text": "{{$cache.components[1].components[2].text}}",
                            "style": {
                              "height": "130"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[1].components[2]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/viewVaultMeme.json",
                                    "memeId": "{{$cache.components[1].components[2].memeId}}"
                                  }
                                }
                              }
                            ]
                          }
                        ]
                      },
                      {
                        "type": "horizontal",
                        "style": {
                    			"padding": "0",
                    			"spacing": "0"
                    		},
                        "components": [
                          {
                            "type": "html",
                            "text": "{{$cache.components[2].components[0].text}}",
                            "style": {
                              "height": "130"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[2].components[0]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/viewVaultMeme.json",
                                    "memeId": "{{$cache.components[2].components[0].memeId}}"
                                  }
                                }
                              }
                            ]
                          },
                          {
                            "type": "html",
                            "text": "{{$cache.components[2].components[1].text}}",
                            "style": {
                              "height": "130"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[2].components[1]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/viewVaultMeme.json",
                                    "memeId": "{{$cache.components[2].components[1].memeId}}"
                                  }
                                }
                              }
                            ]
                          },
                          {
                            "type": "html",
                            "text": "{{$cache.components[2].components[2].text}}",
                            "style": {
                              "height": "130"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[2].components[2]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/viewVaultMeme.json",
                                    "memeId": "{{$cache.components[2].components[2].memeId}}"
                                  }
                                }
                              }
                            ]
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }]
        }
      }
    }
  }
}
