{
  "$jason": {
    "head": {
      "data": {
        "load-phrases": [
          { "phrase": "?" }
        ]
      },
      "title": "Empty",
      "actions": {
        "$show": {
          "type": "$global.set",
          "options": {
            "test": "bla"
          },
          "success": {
            "type": "$cache.set",
            "options": [
              {
                "{{#if 'test' in $global}}": {
                  "test": "test in global"
                }
              },
              {
                "{{#else}}": {
                  "test": "test not in global"
                }
              }
            ],
            "success": {
              "type": "$render"
            }
          }
        }
      },
      "templates": {
        "body": {
          "layers": [
            {
              "type": "image",
              "url": "https://collectmemes.com/app-images-new/logo.png",
              "style": {
                "width": "300",
                "height": "100",
                "top": "50%-50",
                "left": "50%-150"
              }
            },
            {
              "type": "label",
              "text": "{{$cache.test}}",
              "style": {
                "width": "80%",
                "height": "100",
                "top": "50%+100",
                "left": "10%",
                "font": "HelveticaNeue-Bold",
                "size": "16",
                "color": "#000000"
              }
            }
          ]
        }
      }
    }
  }
}
