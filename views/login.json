{
  "$jason": {
    "head": {
      "title": "login"
    },
    "body": {
      "background": "#ffffff",
      "layers": [
        {
          "type": "image",
          "url": "https://collectmemes.com/app-images-new/logo.png",
          "style": {
            "width": "300",
            "height": "100",
            "top": "25%-100",
            "left": "50%-150"
          }
        },
        {
          "type": "label",
          "text": "Login",
          "style": {
            "color": "#111111",
            "align": "center",
            "width": "300",
            "left": "50%-150",
            "top": "25%+5",
            "size": "25"
          }
        },
        {
          "type": "label",
          "text": "to an existing account.",
          "style": {
            "color": "#b8b8b8",
            "align": "center",
            "width": "300",
            "left": "50%-150",
            "top": "25%+40",
            "size": "15"
          }
        },
        {
          "type": "textfield",
          "name": "username",
          "placeholder": "username...",
          "style": {
            "font": "Roboto",
            "size": "20",
            "color": "#fefefe",
            "background": "#eff0f2",
            "padding": "20",
            "top": "38%",
            "width": "80%",
            "left": "10%",
            "corner_radius": "30"
          }
        },{
          "type": "textfield",
          "name": "password",
          "placeholder": "password...",
          "style": {
            "font": "Roboto",
            "size": "20",
            "color": "#fefefe",
            "background": "#eff0f2",
            "padding": "20",
            "secure": "true",
            "top": "50%",
            "width": "80%",
            "left": "10%",
            "corner_radius": "30"
          }
        },
        { "type": "label",
          "text": "Sign in",
          "action": {
            "type": "$href",
            "options": {
              "url": "https://collectmemes.com/views/login.json",
              "transition": "push"
            }
          },
          "style": {
            "width": "300",
            "height": "55",
            "size": "17",
            "top": "65%",
            "left": "50%-150",
            "background": "#2d91d1",
            "color": "#ffffff",
            "padding": "2",
            "corner_radius": "50",
            "align": "center"
          }
        },
        {
          "type": "label",
          "text": "Go Back",
          "style": {
            "color": "#b8b8b8",
            "align": "left",
            "left": "2%",
            "width": "10%",
            "top": "2%",
            "size": "15"
          }
        },
        {
          "type": "label",
          "text": "Forgot password?",
          "style": {
            "color": "#b8b8b8",
            "align": "right",
            "width": "300",
            "left": "50%-150",
            "top": "65%+70",
            "size": "15"
          }
        }
      ]
    }
  }
}
