{
  "$jason": {
    "head": {
      "title": "Collect",
      "actions": {
        "mainLoad": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/devandroidaccess/getCollect.php",
            "method": "post",
            "data": {
              "userId": "{{$global.userKey}}",
              "pass": "933kfjhga7862344bv",
              "scheme": "light",
              "spin": [
                {
                  "{{#if 'spin' in $params}}": "true"
                },
                {
                  "{{#else}}": "false"
                }
              ]
            }
          },
          "success": [
            {
              "{{#if $jason.update == 1}}": {
                "type": "$cache.set",
                "options": {
                  "cur": "{{$jason.cur}}",
                  "achievement": "{{$jason.achievement}}",
                  "heart": "{{$jason.heart}}",
                  "currentSpinButton": "file://collect/spin.png",
                  "currentCollectButton": "file://collect/collect.png"
                },
                "success": [
                  {
                    "{{#if $cache.achievement.status == 1}}": {
                      "type": "$render",
                      "options": {
                        "template": "achievement"
                      }
                    }
                  },
                  {
                    "{{#else}}": {
                      "type": "$render",
                      "options": {
                        "template": "achievement"
                      }
                    }
                  }
                ]
              }
            },
            {
              "{{#else}}": {
                "type": "$cache.set",
                "options": {
                  "currentSpinButton": "file://collect/spin.png",
                  "currentCollectButton": "file://collect/collect.png"
                },
                "success": {
                  "type": "$render",
                  "options": {
                    "template": "achievement"
                  }
                }
              }
            }
          ]
        },
        "userCheck": [
          {
            "{{#if $global.userKey != '0'}}": {
              "trigger": "mainLoad"
            }
          },
          {
            "{{#else}}": {
              "type": "$href",
              "options": {
                "url": "https://collectmemes.com/devandroidviews/welcome.json"
              }
            }
          }
        ],
        "$load": [
          {
            "{{#if 'userKey' in $global}}": {
              "trigger": "userCheck"
            }
          },
          {
            "{{#else}}": {
              "type": "$global.set",
              "options": {
                "userKey": "0"
              },
              "success": {
                "trigger": "userCheck"
              }
            }
          }
        ],
        "onSpin": {
          "trigger": "interactSpinButton",
          "success": {
            "type": "$href",
            "options": {
              "url": "https://collectmemes.com/devandroidviews/collect.json",
              "transition": "fullscreen",
              "options": {
                "call": "true"
              }
            }
          }
        },
        "interactCollectButton": {
          "type": "$cache.set"
        },
        "onCollect": {
          "trigger": "interactCollectButton",
          "success": {
            "type": "$network.request",
            "options": {
              "url": "https://collectmemes.com/devandroidaccess/addToCollection.php",
              "method": "post",
              "data": {
                "memeId": "{{$cache.cur.memeId}}",
                "userId": "{{$global.userKey}}"
              }
            },
            "success": [
              {
                "{{#if $jason.achievement.status == 1}}": {
                  "type": "$cache.set",
                  "options": {
                    "achievement": "{{$jason.achievement}}"
                  },
                  "success": {
                    "type": "$render",
                    "options": {
                      "template": "achievement"
                    }
                  }
                }
              },
              {
                "{{#else}}": {
                  "type": "$render",
                  "options": {
                    "template": "memeCollected"
                  }
                }
              }
            ]
          }
        }
      },
      "templates": {
        "welcome": {
          "background": "#ffffff"
        },
        "achievement": {
          "background": "#ffffff",
          "layers": [
            {
              "type": "image",
              "url": "file://collect/achievement-top.png",
              "style": {
                "top": "-10",
                "left": "0",
                "width": "100%",
                "height": "42%"
              }
            },
            {
              "type": "image",
              "url": "{{$cache.achievement.image}}",
              "style": {
                "top": "5%",
                "width": "58%",
                "left": "21%"
              }
            },
            {
              "type": "label",
              "text": "{{$cache.achievement.title}}",
              "style": {
                "top": "45%",
                "left": "10%",
                "width": "80%",
                "size": "35",
                "color": "#111111"
              }
            },
            {
              "type": "label",
              "text": "{{$cache.achievement.reqs}}",
              "style": {
                "top": "45%+40",
                "left": "10%",
                "width": "80%",
                "size": "25",
                "color": "#111111"
              }
            },
            {
              "type": "label",
              "text": "{{$cache.achievement.xp}}",
              "style": {
                "top": "45%+60",
                "left": "10%",
                "width": "80%",
                "size": "25",
                "color": "2d91d1"
              }
            },
            {
              "type": "image",
              "url": "{{$cache.achievement.emmaImage}}",
              "style": {
                "left": "0",
                "bottom": "0",
                "width": "45%"
              }
            },
            {
              "type": "label",
              "text": "{{$cache.achievement.emmaQuote}}",
              "style": {
                "width": "50%",
                "bottom": "20%",
                "right": "5",
                "color": "#111111",
                "size": "25"
              }
            },
            {
              "type": "image",
              "url": "file://collect/continue.png",
              "style": {
                "right": "5",
                "bottom": "5",
                "width": "50%"
              },
              "action": {
                "type": "$render",
                "options": {
                  "template": "{{$cache.achievement.nextTemplate}}"
                }
              }
            },
            {
              "type": "label",
              "text": "refresh",
              "style": {
                "top": "40",
                "left": "40"
              },
              "action": {
                "type": "$href",
                "options": {
                  "url": "https://collectmemes.com/devandroidviews/collect.json",
                  "transition": "fullscreen"
                }
              }
            }
          ]
        },
        "memeCollected": {
          "background": "#ffffff"
        },
        "body": {
          "background": "#ffffff",
          "layers": [
            {
              "type": "label",
              "text": "refresh",
              "style": {
                "top": "40",
                "left": "40"
              },
              "action": {
                "type": "$href",
                "options": {
                  "url": "https://collectmemes.com/devandroidviews/collect.json",
                  "transition": "fullscreen"
                }
              }
            },
            {
              "type": "image",
              "url": "{{$cache.cur.rarityLining}}",
              "style": {
                "top": "-10",
                "left": "0",
                "width": "100%",
                "height": "4%"
              }
            },
            {
              "type": "image",
              "url": "{{$cache.cur.rarityLining}}",
              "style": {
                "bottom": "-10",
                "left": "0",
                "width": "100%",
                "height": "4%"
              }
            },
            {
              "type": "image",
              "url": "{{$cache.cur.rarityImage}}",
              "style": {
                "left": "31%",
                "top": "3%",
                "width": "14%",
                "height": "7%"
              }
            },
            {
              "type": "label",
              "text": "Rank: #{{$cache.cur.rank}}",
              "style": {
                "top": "3%+3",
                "left": "47%+2",
                "size": "17",
                "color": "#111111",
                "width": "30%",
                "height": "3%"
              }
            },
            {
              "type": "label",
              "text": "{{$cache.cur.rarity}}",
              "style": {
                "top": "6%",
                "left": "47%+2",
                "size": "17",
                "color": "{{$cache.cur.rarityColor}}",
                "width": "30%",
                "height": "3%"
              }
            },
            {
              "type": "label",
              "text": "{{$cache.cur.title}}",
              "style": {
                "left": "3%",
                "top": "10%",
                "width": "94%",
                "height": "6%",
                "align": "center",
                "color": "#111111",
                "size": "35"
              }
            },
            {
              "type": "label",
              "text": "by {{$cache.cur.author}}",
              "style": {
                "left": "3%",
                "top": "10%+40",
                "width": "94%",
                "height": "5%",
                "align": "center",
                "color": "#111111",
                "size": "20"
              }
            },
            {
              "type": "image",
              "url": "{{$cache.cur.image}}",
              "style": {
                "top": "15%+50",
                "left": "8%",
                "width": "84%"
              }
            },
            {
              "type": "image",
              "url": "{{$cache.heart}}",
              "style": {
                "top": "59%+60",
                "left": "8%",
                "width": "15%"
              },
              "action": [
                {
                  "{{#if $cache.cur.liked == 1}}": {
                    "type": "$network.request",
                    "options": {
                      "url": "https://collectmemes.com/devandroidaccess/unlikeMeme.php",
                      "method": "post",
                      "data": {
                        "userId": "{{$global.userKey}}",
                        "memeId": "{{$cache.cur.memeId}}",
                        "scheme": "light"
                      }
                    },
                    "success": {
                      "type": "$cache.set",
                      "options": {
                        "heart": "{{$jason.heart}}"
                      },
                      "success": {
                        "type": "$render"
                      }
                    }
                  }
                },
                {
                  "{{#else}}": {
                    "type": "$network.request",
                    "options": {
                      "url": "https://collectmemes.com/devandroidaccess/likeMeme.php",
                      "method": "post",
                      "data": {
                        "userId": "{{$global.userKey}}",
                        "memeId": "{{$cache.cur.memeId}}"
                      }
                    },
                    "success": {
                      "type": "$cache.set",
                      "options": {
                        "heart": "file://shared/heart-full.png"
                      },
                      "success": {
                        "type": "$render"
                      }
                    }
                  }
                }
              ]
            },
            {
              "type": "image",
              "url": "{{$cache.currentSpinButton}}",
              "style": {
                "top": "70%+50",
                "width": "42%",
                "left": "6%",
                "height": "8%"
              },
              "action": {
                "type": "$href",
                "options": {
                  "url": "https://collectmemes.com/devandroidviews/collect.json",
                  "transition": "fullscreen",
                  "options": {
                    "spin": "true"
                  }
                }
              }
            },
            {
              "type": "image",
              "url": "{{$cache.currentCollectButton}}",
              "style": {
                "top": "70%+50",
                "width": "42%",
                "left": "52%",
                "height": "8%"
              },
              "action": {
                "type": "$network.requet",
                "options": {
                  "url": "https://collectmemes.com/devandroidaccess/addToCollection.php",
                  "method": "post",
                  "data": {
                    "userId": "{{$global.userKey}}",
                    "memeId": "{{$cache.cur.memeId}}"
                  }
                },
                "success": {
                  "type": "$cache.set",
                  "options": {
                    "achievement": "{{$jason.achievement}}",
                    "nextTemplate": "{{$jason.nextTemplate}}"
                  },
                  "success": {
                    "type": "$render",
                    "options": {
                      "template": "{{$cache.nextTemplate}}"
                    }
                  }
                }
              }
            },
            {
              "type": "html",
              "text": "{{$cache.cur.spinMessage}}",
              "style": {
                "height": "5%",
                "top": "80%+55",
                "width": "42%",
                "left": "6%"
              }
            },
            {
              "type": "image",
              "url": [
                {
                  "{{#if 1}}": "file://collect/collection-light.png"
                },
                {
                  "{{#else}}": "file://collect/collection-dark.png"
                }
              ],
              "style": {
                "top": "80%+60",
                "width": "6%",
                "left": "70%"
              },
              "action": {
                "type": "$href",
                "options": {
                  "url": "https://collectmemes.com/devandroidviews/collection.json",
                  "transition": "fullscreen"
                }
              }
            }
          ]
        }
      }
    }
  }
}
