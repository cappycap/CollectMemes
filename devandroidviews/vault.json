{
  "$jason": {
    "head": {
      "data": {
        "load-phrases": [
          { "phrase": "?" }
        ]
      },
      "title": "Vault",
      "actions": {
        "mainLoad": [
          {
            "{{#if 'cur' in $cache}}": {
              "trigger": "fetchImages"
            }
          },
          {
            "{{#else}}": {
              "type": "$cache.set",
              "options": {
                "cur": "1",
                "sort": "rank",
                "sortDir": "1"
              },
              "success": {
                "trigger": "fetchImages"
              }
            }
          }
        ],
        "deletedCheck": [
          {
            "{{#if 'memeRemoved' in $global}}": [
              {
                "{{#if $global.memeRemoved == 0}}": {
                  "trigger": "mainLoad"
                }
              },
              {
                "{{#else}}": {
                  "type": "$util.banner",
                  "options": {
                    "title": "Attention:",
                    "description": "Meme removed from collection!"
                  },
                  "success": {
                    "type": "$global.set",
                    "options": {
                      "memeRemoved": "0"
                    },
                    "success": {
                      "type": "$cache.set",
                      "options": {
                        "cur": "1"
                      },
                      "success": {
                        "trigger": "mainLoad"
                      }
                    }
                  }
                }
              }
            ]
          },
          {
            "{{#else}}": {
              "trigger": "mainLoad"
            }
          }
        ],
        "$load": [
          {
            "{{#if $global.userKey != '0'}}": {
              "type": "$network.request",
              "options": {
                "url": "https://collectmemes.com/devandroidaccess/memeHeight.php",
                "method": "post",
                "data": {
                  "width": "{{$env.device.width}}"
                }
              },
              "success": {
                "type": "$cache.set",
                "options": {
                  "memeHeight": "{{$jason.ret}}"
                },
                "success": {
                  "trigger": "deletedCheck"
                }
              }
            }
          },
          {
            "{{#else}}": {
              "type": "$href",
              "options": {
                "url": "https://collectmemes.com/devandroidviews/welcome.json"
              }
            }
          }
        ],
        "fetchImages": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/devandroidaccess/getVaultCollection.php",
            "method": "post",
            "data": {
              "userId": "{{$global.userKey}}",
              "cur": "{{$cache.cur}}",
              "sort": "{{$cache.sort}}",
              "sortDir": "{{$cache.sortDir}}",
              "scheme": "light"
            }
          },
          "success": {
            "type": "$cache.set",
            "options": {
              "components": "{{$jason.components}}",
              "stats": "{{$jason.stats}}",
              "nav": "{{$jason.nav}}"
            },
            "success": {
              "type": "$render"
            }
          }
        },
        "onRank": {
          "type": "$cache.set",
          "options": {
            "sort": "rank"
          },
          "success": {
            "type": "$reload"
          }
        },
        "onRecent": {
          "type": "$cache.set",
          "options": {
            "sort": "dateAdded"
          },
          "success": {
            "type": "$reload"
          }
        },
        "onSort": {
          "type": "$cache.set",
          "options": {
            "sortDir": [
              {
                "{{#if $cache.sortDir == 1}}": "0"
              },
              {
                "{{#else}}": "1"
              }
            ]
          },
          "success": {
            "type": "$reload"
          }
        }
      },
      "templates": {
        "body": {
          "background": "file://bg.png",
          "style": {
            "border": "none"
          },
          "sections": [{
            "items": [
              {
                "{{#if $cache.components == 'undefined'}}": [
                  {
                    "type": "label",
                    "text": "No memes collected yet!",
                    "style": {
                      "padding": "20",
                      "align": "center",
                      "color": "#111111",
                      "size": "25"
                    }
                  }
                ]
              },
              {
                "{{#else}}": [
                  {
                    "type": "image",
                    "url": "file://shared/lining.png",
                    "style": {
                      "width": "100%"
                    }
                  },
                  {
                    "type": "vertical",
                    "style": {
                      "align": "center",
                      "padding": "5"
                    }
                    "components": [
                      {
                        "type": "horizontal",
                        "style": {
                          "align": "center"
                        }
                        "components": [
                          {
                            "type": "image",
                            "url": "{{$cache.nav.collectLeft}}"
                          },
                          {
                            "type": "space"
                          },
                          {
                            "type": "image",
                            "url": "file://tops/collection.png",
                            "style": {
                              "padding": "10",
                              "width": "15%"
                            }
                          },
                          {
                            "type": "space"
                          },
                          {
                            "type": "image",
                            "url": "{{$cache.nav.likesRight}}"
                          },
                        ]
                      },
                      {
                        "type": "label",
                        "text": "Vault",
                        "style": {
                          "padding": "5",
                          "size": "40",
                          "color": "#111111"
                        }
                      },
                      {
                        "type": "horizontal",
                        "style": {
                          "align": "center"
                        }
                        "components": [
                          {
                            "type": "space"
                          },
                          {
                            "type": "html",
                            "url": "{{$cache.stats}}",
                            "style": {
                              "padding": "10",
                              "width": "15%"
                            }
                          },
                          {
                            "type": "image",
                            "url": "{{$cache.nav.sortButton}}"
                          },
                        ]
                      },
                    ]
                  },
                  {
                    "type": "vertical",
                    "style": {
                      "padding": "0",
                      "spacing": "0"
                    },
                    "components": [
                      {
                        "type": "horizontal",
                        "style": {
                    			"padding": "0",
                    			"spacing": "0"
                    		},
                        "components": [
                          {
                            "type": "html",
                            "text": "{{$cache.components[0].components[0].text}}",
                            "style": {
                              "height": "{{$cache.memeHeight}}"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[0].components[0]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/devandroidviews/viewVaultMeme.json",
                                    "options": {
                                      "memeId": "{{$cache.components[0].components[0].memeId}}"
                                    }
                                  }
                                }
                              }
                            ]
                          },
                          {
                            "type": "html",
                            "text": "{{$cache.components[0].components[1].text}}",
                            "style": {
                              "height": "{{$cache.memeHeight}}"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[0].components[1]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/devandroidviews/viewVaultMeme.json",
                                    "options": {
                                      "memeId": "{{$cache.components[0].components[1].memeId}}"
                                    }
                                  }
                                }
                              }
                            ]
                          },
                          {
                            "type": "html",
                            "text": "{{$cache.components[0].components[2].text}}",
                            "style": {
                              "height": "{{$cache.memeHeight}}"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[0].components[2]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/devandroidviews/viewVaultMeme.json",
                                    "options": {
                                      "memeId": "{{$cache.components[0].components[2].memeId}}"
                                    }
                                  }
                                }
                              }
                            ]
                          }
                        ]
                      },
                      {
                        "type": "horizontal",
                        "style": {
                    			"padding": "0",
                    			"spacing": "0"
                    		},
                        "components": [
                          {
                            "type": "html",
                            "text": "{{$cache.components[1].components[0].text}}",
                            "style": {
                              "height": "{{$cache.memeHeight}}"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[1].components[0]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/devandroidviews/viewVaultMeme.json",
                                    "options": {
                                      "memeId": "{{$cache.components[1].components[0].memeId}}"
                                    }
                                  }
                                }
                              }
                            ]
                          },
                          {
                            "type": "html",
                            "text": "{{$cache.components[1].components[1].text}}",
                            "style": {
                              "height": "{{$cache.memeHeight}}"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[1].components[1]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/devandroidviews/viewVaultMeme.json",
                                    "options": {
                                      "memeId": "{{$cache.components[1].components[1].memeId}}"
                                    }
                                  }
                                }
                              }
                            ]
                          },
                          {
                            "type": "html",
                            "text": "{{$cache.components[1].components[2].text}}",
                            "style": {
                              "height": "{{$cache.memeHeight}}"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[1].components[2]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/devandroidviews/viewVaultMeme.json",
                                    "options": {
                                      "memeId": "{{$cache.components[1].components[2].memeId}}"
                                    }
                                  }
                                }
                              }
                            ]
                          }
                        ]
                      },
                      {
                        "type": "horizontal",
                        "style": {
                    			"padding": "0",
                    			"spacing": "0"
                    		},
                        "components": [
                          {
                            "type": "html",
                            "text": "{{$cache.components[2].components[0].text}}",
                            "style": {
                              "height": "{{$cache.memeHeight}}"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[2].components[0]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/devandroidviews/viewVaultMeme.json",
                                    "options": {
                                      "memeId": "{{$cache.components[2].components[0].memeId}}"
                                    }
                                  }
                                }
                              }
                            ]
                          },
                          {
                            "type": "html",
                            "text": "{{$cache.components[2].components[1].text}}",
                            "style": {
                              "height": "{{$cache.memeHeight}}"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[2].components[1]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/devandroidviews/viewVaultMeme.json",
                                    "options": {
                                      "memeId": "{{$cache.components[2].components[1].memeId}}"
                                    }
                                  }
                                }
                              }
                            ]
                          },
                          {
                            "type": "html",
                            "text": "{{$cache.components[2].components[2].text}}",
                            "style": {
                              "height": "{{$cache.memeHeight}}"
                            },
                            "action": [
                              {
                                "{{#if 'memeId' in $cache.components[2].components[2]}}": {
                                  "type": "$href",
                                  "options": {
                                    "url": "https://collectmemes.com/devandroidviews/viewVaultMeme.json",
                                    "options": {
                                      "memeId": "{{$cache.components[2].components[2].memeId}}"
                                    }
                                  }
                                }
                              }
                            ]
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }]
        }
      }
    }
  }
}
