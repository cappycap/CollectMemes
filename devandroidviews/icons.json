                    {
                      "type": "image",
                      "url": "file://shared/lining.png",
                      "style": {
                        "width": "100%"
                      }
                    },
                    {
                      "type": "space",
                      "style": {
                        "height": "1%"
                      }
                    },
                    {
                      "type": "horizontal",
                      "style": {
                        "align": "center",
                        "width": "100%",
                        "padding": "10",
                        "spacing": "10",
                        "height": "10%"
                      },
                      "components": [
                        {
                          "type": "image",
                          "url": [
                            {
                              "{{#if $global.scheme == 'light'}}": "file://nav/tap-left-light.png"
                            },
                            {
                              "{{#else}}": "file://nav/tap-left-dark.png"
                            }
                          ],
                          "style": {
                            "width": "12%"
                          },
                          "action": {
                            "type": "$href",
                            "options": {
                              "url": "https://collectmemes.com/devandroidviews/settings.json",
                              "transition": "fullscreen"
                            }
                          }
                        },
                        {
