{
  "$jason": {
    "head": {
      "title": "Welcome",
      "actions": {
        "updateIcon": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/devandroidaccess/updateIcon.php",
            "method": "post",
            "data": {
              "userId": "{{$global.userKey}}",
              "iconId": "{{$params.iconId}}"
            }
          },
          "success": {
            "type": "$href",
            "options": {
              "url": "https://collectmemes.com/devandroidviews/settings.json",
              "transition": "fullscreen",
              "options": {
                "alert": "true",
                "title": "Success!",
                "description": "Your new icon has been set!"
              }
            }
          }
        },
        "mainLoad": [
          {
            "{{#if 'iconId' in $params}}": {
              "trigger": "updateIcon"
            }
          },
          {
            "{{#else}}": {
              "type": "$render"
            }
          }
        ],
        "$load": [
          {
            "{{#if $global.userKey != '0'}}": {
              "trigger": "mainLoad"
            }
          },
          {
            "{{#else}}": {
              "type": "$href",
              "options": {
                "url": "https://collectmemes.com/devandroidviews/collect.json"
              }
            }
          }
        ]
      },
      "templates": {
        "body": {
          "background": [{"{{#if $global.scheme == 'light'}}":"#ffffff"},{"{{#else}}":"#111111"}],
