{
  "$jason": {
    "head": {
      "title": "Collect",
      "actions": {
        "mainLoad": [{
          "{{#if $global.firstLoadDone == 0}}": {
            "type": "$cache.set",
            "options": {
              "collectStatus": "1",
              "collected": "0"
            },
            "success": {
              "type": "$network.request",
              "options": {
                "url": "https://collectmemes.com/access/getMeme.php",
                "method": "post",
                "data": {
                  "userId": "{{$global.userKey}}",
                  "pass": "933kfjhga7862344bv",
                  "spinsLeft": "{{$global.spinsLeft}}"
                }
              },
              "success": {
                "type": "$cache.set",
                "options": {
                  "current": "{{$jason}}",
                  "liked": "{{$jason.liked}}",
                  "failedSpin": "0"
                },
                "success": {
                  "type": "$cache.set",
                  "options": {
                    "spinStatus": "1"
                  },
                  "success": {
                    "type": "$network.request",
                    "options": {
                      "url": "https://collectmemes.com/access/checkTime.php",
                      "method": "post",
                      "data": {
                        "sub": "1"
                      }
                    },
                    "success": {
                      "type": "$global.set",
                      "options": {
                        "nextSpin": "{{$jason.time}}"
                      },
                      "success": {
                        "type": "$global.set",
                        "options": {
                          "firstLoadDone": "1"
                        },
                        "success": {
                          "type": "$render",
                          "options": {
                            "template": "welcome"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        {
          "{{#else}}": [{
              "{{#if $cache.collectStatus == 1}}": {
                "type": "$render"
              }
            },
            {
              "{{#else}}": {
                "type": "$network.request",
                "options": {
                  "url": "https://collectmemes.com/access/checkTime.php"
                },
                "success": [{
                  "{{#if $jason.time > $global.nextSpin}}": {
                    "type": "$cache.set",
                    "options": {
                      "collectStatus": "1"
                    },
                    "success": {
                      "type": "$cache.set",
                      "options": {
                        "spinStatus": "1",
                        "current": {
                          "id": "-1",
                          "title": "Spin to get started!",
                          "image": "file://start.png",
                          "top": "file://c-common-top.png",
                          "bot": "file://c-common-bot.png"
                        }
                      },
                      "success": {
                        "type": "$global.set",
                        "options": {
                          "spinsLeft": "10"
                        },
                        "success": {
                          "type": "$render"
                        }
                      }
                    }
                  }
                },
                {
                  "{{#else}}": [
                    {
                      "{{#if $cache.collected == 1}}": {
                        "type": "$util.banner",
                        "options": {
                          "title": "Meme Collected!",
                          "description": "{{$cache.bannerDescription}}"
                        },
                        "success": {
                          "type": "$cache.set",
                          "options": {
                            "collected": "0"
                          },
                          "success": [
                            {
                              "{{#if $cache.achievement == -1}}": {
                                "type": "$render"
                              }
                            },
                            {
                              "{{#else}}": {
                                "type": "$render",
                                "options": {
                                  "template": "achievement"
                                }
                              }
                            }
                          ]
                        }
                      }
                    },
                    {
                      "{{#else}}": [
                        {
                          "{{#if $cache.failedSpin == 1}}": {
                            "type": "$network.request",
                            "options": {
                              "url": "https://collectmemes.com/access/failedSpin.php",
                              "method": "post",
                              "data": {
                                "nextSpin": "{{$global.nextSpin}}"
                              }
                            },
                            "success": {
                              "type": "$util.banner",
                              "options": {
                                "title": "Wait a bit longer...",
                                "description": "{{$jason.message}}"
                              },
                              "success": {
                                "type": "$cache.set",
                                "options": {
                                  "failedSpin": "0"
                                },
                                "success": {
                                  "type": "$render"
                                }
                              }
                            }
                          }
                        },
                        {
                          "{{#else}}": {
                            "type": "$render"
                          }
                        }
                      ]
                    }
                  ]
                }
                ]
              }
            }
          ]
        }],
        "yeetLoad": [
          {
            "{{#if $global.userKey != '0'}}": {
              "trigger": "mainLoad"
            }
          },
          {
            "{{#else}}": {
              "type": "$href",
              "options": {
                "url": "https://collectmemes.com/views/welcome.json"
              }
            }
          }
        ],
        "$load": [
          {
            "{{#if 'userKey' in $global}}": {
              "trigger": "yeetLoad"
            }
          },
          {
            "{{#else}}": {
              "type": "$global.set",
              "options": {
                "userKey": "0"
              },
              "success": {
                "trigger": "yeetLoad"
              }
            }
          }
        ],
        "onSpin": {
          "type": "$global.set",
          "options": {
            "spinsLeft": "{{$global.spinsLeft - 1}}"
          },
          "success": {
            "type": "$network.request",
            "options": {
              "url": "https://collectmemes.com/access/getMeme.php",
              "method": "post",
              "data": {
                "userId": "{{$global.userKey}}",
                "pass": "933kfjhga7862344bv",
                "spinsLeft": "{{$global.spinsLeft}}"
              }
            },
            "success": {
              "type": "$cache.set",
              "options": {
                "current": "{{$jason}}",
                "liked": "{{$jason.liked}}"
              },
              "success": [
                {
                  "{{#if $global.spinsLeft == 0}}": {
                    "type": "$cache.set",
                    "options": {
                      "spinStatus": "0"
                    },
                    "success": {
                      "type": "$network.request",
                      "options": {
                        "url": "https://collectmemes.com/access/checkTime.php",
                        "method": "post",
                        "data": {
                          "add": "3599"
                        }
                      },
                      "success": {
                        "type": "$global.set",
                        "options": {
                          "nextSpin": "{{$jason.time}}"
                        },
                        "success": {
                          "type": "$render"
                        }
                      }
                    }
                  }
                },
                {
                  "{{#else}}": {
                    "type": "$render"
                  }
                }
              ]
            }
          }
        },
        "onCollect": [
          {
            "{{#if $cache.collectStatus == 1}}": {
              "type": "$network.request",
              "options": {
                "url": "https://collectmemes.com/access/checkTime.php",
                "method": "post",
                "data": {
                  "add": "3599"
                }
              },
              "success": {
                "type": "$global.set",
                "options": {
                  "nextSpin": "{{$jason.time}}"
                },
                "success": {
                  "type": "$cache.set",
                  "options": {
                    "collectStatus": "0",
                    "spinStatus": "0"
                  },
                  "success": {
                    "type": "$network.request",
                    "options": {
                      "url": "https://collectmemes.com/access/addToCollection.php",
                      "method": "post",
                      "data": {
                        "userId": "{{$global.userKey}}",
                        "memeId": "{{$cache.current.id}}",
                        "spinsLeft": "{{$global.spinsLeft}}"
                      }
                    },
                    "success": {
                      "type": "$cache.set",
                      "options": {
                        "collected": "1"
                      },
                      "success": {
                        "type": "$global.set",
                        "options": {
                          "spinsLeft": "0"
                        },
                        "success": {
                          "type": "$reload"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        ],
        "likeMeme": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/access/likeMeme.php",
            "method": "post",
            "data": {
              "userId": "{{$global.userKey}}",
              "memeId": "{{$cache.current.id}}"
            }
          },
          "success": {
            "type": "$cache.set",
            "options": {
              "liked": "1"
            },
            "success": {
              "type": "$render"
            }
          }
        },
        "unlikeMeme": {
          "type": "$network.request",
          "options": {
            "url": "https://collectmemes.com/access/unlikeMeme.php",
            "method": "post",
            "data": {
              "userId": "{{$global.userKey}}",
              "memeId": "{{$cache.current.id}}"
            }
          },
          "success": {
            "type": "$cache.set",
            "options": {
              "liked": "0"
            },
            "success": {
              "type": "$render"
            }
          }
        }
      },
      "templates": {
        "welcome": {
          "background": "#ffffff",
          "layers": [
            {
              "type": "image",
              "url": "https://collectmemes.com/img/logo.png",
              "style": {
                "width": "300",
                "height": "100",
                "top": "30%-100",
                "left": "50%-150"
              }
            },
            {
              "type": "label",
              "text": "Welcome to CollectMemes!",
              "style": {
                "color": "#111111",
                "align": "center",
                "width": "300",
                "left": "50%-150",
                "top": "30%+5",
                "size": "25"
              }
            },
            {
              "type": "label",
              "text": "We're currently in Alpha, which means there may be some fun bugs you encounter. Help us improve later versions by reporting them! feedback@collectmemes.com",
              "style": {
                "color": "#b8b8b8",
                "align": "center",
                "width": "70%",
                "left": "15%",
                "top": "30%+75",
                "size": "15"
              }
            },
            {
              "type": "label",
              "text": "Happy spinning!",
              "style": {
                "color": "#b8b8b8",
                "align": "center",
                "width": "70%",
                "left": "15%",
                "top": "30%+200",
                "size": "15"
              }
            },
            { "type": "label",
              "text": "Tap to start!",
              "action": {
                "type": "$href",
                "options": {
                  "url": "https://collectmemes.com/views/collect.json",
                  "transition": "push"
                }
              },
              "style": {
                "width": "300",
                "height": "55",
                "size": "17",
                "top": "30%+300",
                "left": "50%-150",
                "background": "#2d91d1",
                "color": "#ffffff",
                "padding": "2",
                "corner_radius": "50",
                "align": "center"
              }
            }
          ]
        },
        "achievement": {
          "background": "#ffffff",
          "layers": [
            {
              "type": "label",
              "text": "Achievement Acquired:",
              "style": {
                "align": "center",
                "color": "#111111",
                "width": "100%",
                "top": "20%",
                "left": "0",
                "size": "25"
              }
            },
            {
              "type": "label",
              "text": "{{$cache.achievementTitle}}",
              "style": {
                "align": "center",
                "color": "#111111",
                "width": "100%",
                "top": "20%+30",
                "left": "0",
                "size": "22"
              }
            },
            {
              "type": "image",
              "url": "{{$cache.achievementImage}}",
              "style": {
                "top": "20%+60",
                "width": "80%",
                "height": "{{$cache.env80}}",
              }
            },
            { "type": "label",
              "text": "OK!",
              "action": {
                "type": "$href",
                "options": {
                  "url": "https://collectmemes.com/views/collect.json",
                  "transition": "push"
                }
              },
              "style": {
                "width": "300",
                "height": "55",
                "size": "17",
                "top": "60%",
                "left": "50%-150",
                "background": "#2d91d1",
                "color": "#ffffff",
                "padding": "2",
                "corner_radius": "50",
                "align": "center"
              }
            }
          ]
        },
        "body": {
          "background": "#ffffff",
          "layers": [
            {
              "type": "image",
              "url": "{{$cache.current.top}}",
              "style": {
                "width": "100%",
                "top": "-60"
              }
            },
            {
              "type": "image",
              "url": "{{$cache.current.bot}}",
              "style": {
                "width": "100%",
                "bottom": "-35"
              }
            },
            {
              "type": "label",
              "text": "{{$cache.current.title}}",
              "style": {
                "size": "30",
                "color": "#ffffff",
                "align": "center",
                "width": "80%",
                "left": "10%",
                "top": "2%-7",
                "padding": "0"
              }
            },
            {
              "type": "label",
              "text": "by {{$cache.current.creator}}",
              "style": {
                "size": "22",
                "color": "#ffffff",
                "align": "center",
                "width": [
                  {
                    "{{#if $global.spinsLeft == 10}}": "0"
                  },
                  {
                    "{{#else}}": "80%"
                  }
                ],
                "left": "10%",
                "top": "2%+27"
              }
            },
            {
              "type": "label",
              "text": [
                {
                  "{{#if $global.spinsLeft == 0}}": "0 spins left"
                },
                {
                  "{{#else}}": "{{$global.spinsLeft}} spins left"
                }
              ],
              "style": {
                "width": "40%",
                "align": "center",
                "top": "2%+57",
                "left": "30%",
                "size": "22",
                "color": [
                  {
                    "{{#if $global.spinsLeft == 0}}": "#e74c3c"
                  },
                  {
                    "{{#else}}": "#097a29"
                  }
                ]
              }
            },
            {
              "type": "image",
              "url": "{{$cache.current.image}}",
              "style": {
                "width": "80%",
                "height": "300",
                "top": "48%-150",
                "left": "10%"
              }
            },
            {
              "type": "label",
              "text": "#{{$cache.current.rank}}",
              "style": {
                "color": "#ffffff",
                "bottom": "13%+22",
                "left": "30%",
                "width": [
                  {
                    "{{#if $global.spinsLeft == 10}}": "0"
                  },
                  {
                    "{{#else}}": "40%"
                  }
                ],
                "align": "center",
                "size": "25"
              }
            },
            {
              "type": "label",
              "text": "({{$cache.current.rarity}})",
              "style": {
                "color": "{{$cache.current.color}}",
                "bottom": "13%",
                "left": "30%",
                "align": "center",
                "width": [
                  {
                    "{{#if $global.spinsLeft == 10}}": "0"
                  },
                  {
                    "{{#else}}": "40%"
                  }
                ],
                "size": "20"
              }
            },
            {
              "type": "image",
              "url": "file://menu.png",
              "style": {
                "width": "40",
                "top": "1%",
                "right": "3%"
              },
              "action": {
                "type": "$href",
                "options": {
                  "url": "https://collectmemes.com/views/profile.json"
                }
              }
            },
            {
              "type": "button",
              "text": "Spin",
              "style": {
                "width": "44%",
                "height": "8%",
                "align": "center",
                "bottom": "3%",
                "left": "5%",
                "background": [
                  {
                    "{{#if $cache.spinStatus == 1}}": "#34495e"
                  },
                  {
                    "{{#else}}": "#1A232E"
                  }
                ],
                "color": "#ffffff",
                "size": "22",
                "corner_radius": "25"
              },
              "action": [
                {
                  "{{#if $cache.spinStatus == 1}}": {
                    "trigger": "onSpin"
                  }
                },
                {
                  "{{#else}}": {
                    "type": "$cache.set",
                    "options": {
                      "failedSpin": "1"
                    },
                    "success": {
                      "type": "$reload"
                    }
                  }
                }
              ]
            },
            {
              "type": "button",
              "text": "Collect!",
              "style": {
                "width": "44%",
                "height": "8%",
                "align": "center",
                "bottom": "3%",
                "right": "5%",
                "background": [
                  {
                    "{{#if $cache.collectStatus == 1}}": "#319CDE"
                  },
                  {
                    "{{#else}}": "#11364D"
                  }
                ],
                "color": "#ffffff",
                "size": "22",
                "corner_radius": "25"
              },
              "action": [
                {
                  "{{#if $cache.collectStatus == 1}}": [
                    {
                      "{{#if $global.spinsLeft == 10}}": {
                        "type": "$util.banner",
                        "options": {
                          "title": "Whoops...",
                          "description": "Spin for a meme before collecting!"
                        }
                      }
                    },
                    {
                      "{{#else}}": {
                        "trigger": "onCollect"
                      }
                    }
                  ]
                },
                {
                  "{{#else}}": {
                    "type": "$cache.set",
                    "options": {
                      "failedSpin": "1"
                    },
                    "success": {
                      "type": "$reload"
                    }
                  }
                }
              ]
            },
            {
              "type": "image",
              "url": [
                {
                  "{{#if $global.spinsLeft == 10}}": "file://heart-white.png"
                },
                {
                  "{{#elseif $cache.liked == 1}}": "file://heart-full.png"
                },
                {
                  "{{#else}}": "file://heart-empty.png"
                }
              ],
              "style": {
                "width": "40",
                "height": "40",
                "top": "15%+5",
                "left": "3%"
              },
              "action": [
                {
                  "{{#if $global.spinsLeft !== 10 && $cache.liked == 0}}": {
                    "trigger": "likeMeme"
                  }
                },
                {
                  "{{#elseif $cache.liked == 1}}": {
                    "trigger": "unlikeMeme"
                  }
                }
              ]
            },
            {
              "type": "image",
              "url": "file://vault.png",
              "style": {
                "width": "40",
                "height": "40",
                "right": "3%",
                "top": "15%+5"
              },
              "action": {
                "type": "$href",
                "options": {
                  "url": "https://collectmemes.com/views/vault.json",
                  "transition": "push"
                }
              }
            }
          ]
        }
      }
    }
  }
}
