{
  "$jason": {
    "head": {
      "title": "Collect",
      "actions": {
        "mainLoad": [
          {
            "{{#if 'call' in $params}}": {
              "type": "$network.request",
              "options": {
                "url": "https://collectmemes.com/access/getMeme.php",
                "method": "post",
                "data": {
                  "userId": "{{$global.userKey}}"
                }
              },
              "success": {
                "type": "$cache.set",
                "options": {
                  "cur": "{{$jason}}"
                },
                "success": {
                  "type": "$render"
                }
              }
            }
          },
          {
            "{{#else}}": {
              "type": "$render"
            }
          }
        ],
        "userCheck": [
          {
            "{{#if $global.userKey != '0'}}": {
              "trigger": "mainLoad"
            }
          },
          {
            "{{#else}}": {
              "type": "$href",
              "options": {
                "url": "https://collectmemes.com/views/welcome.json"
              }
            }
          }
        ],
        "$load": [
          {
            "{{#if 'userKey' in $global}}": {
              "trigger": "userCheck"
            }
          },
          {
            "{{#else}}": {
              "type": "$global.set",
              "options": {
                "userKey": "0"
              },
              "success": {
                "trigger": "userCheck"
              }
            }
          }
        ]
      },
      "templates": {
        "welcome": {
          "background": "#ffffff"
        },
        "achievement": {
          "background": "#ffffff"
        },
        "body": {
          "background": "#ffffff"
        }
      }
    }
  }
}
