{
  "$jason": {
    "head": {
      "title": "Collect",
      "actions": {
        "mainLoad": [
          {
            "{{#if 'call' in $params || $cache.cur.collectStatus == 0}}": {
              "type": "$network.request",
              "options": {
                "url": "https://collectmemes.com/access/getCollect.php",
                "method": "post",
                "data": {
                  "userId": "{{$global.userKey}}",
                  "pass": "933kfjhga7862344bv"
                }
              },
              "success": {
                "type": "$cache.set",
                "options": {
                  "cur": "{{$jason.cur}}",
                  "achievement": "{{$jason.achievement}}"
                },
                "success": [
                  {
                    "{{#if $cache.achievement.status == 1}}": {
                      "type": "$render",
                      "options": {
                        "template": "achievement"
                      }
                    }
                  },
                  {
                    "{{#else}}": {
                      "type": "$render"
                    }
                  }
                ]
              }
            }
          },
          {
            "{{#else}}": {
              "type": "$render"
            }
          }
        ],
        "userCheck": [
          {
            "{{#if $global.userKey != '0'}}": {
              "trigger": "mainLoad"
            }
          },
          {
            "{{#else}}": {
              "type": "$href",
              "options": {
                "url": "https://collectmemes.com/views/welcome.json"
              }
            }
          }
        ],
        "$load": [
          {
            "{{#if 'userKey' in $global}}": {
              "trigger": "userCheck"
            }
          },
          {
            "{{#else}}": {
              "type": "$global.set",
              "options": {
                "userKey": "0"
              },
              "success": {
                "trigger": "userCheck"
              }
            }
          }
        ],
        "onSpin": {
          "trigger": "interactSpinButton",
          "success": {
            "type": "$href",
            "options": {
              "url": "https://collectmemes.com/views/collect.json",
              "transition": "fullscreen",
              "options": {
                "call": "true"
              }
            }
          }
        },
        "interactCollectButton": {
          "type": "$cache.set"
        },
        "onCollect": {
          "trigger": "interactCollectButton",
          "success": {
            "type": "$network.request",
            "options": {
              "url": "https://collectmemes.com/access/addToCollection.php",
              "method": "post",
              "data": {
                "memeId": "{{$cache.cur.memeId}}",
                "userId": "{{$global.userKey}}"
              }
            },
            "success": [
              {
                "{{#if $jason.achievement.status == 1}}": {
                  "type": "$cache.set",
                  "options": {
                    "achievement": "{{$jason.achievement}}"
                  },
                  "success": {
                    "type": "$render",
                    "options": {
                      "template": "achievement"
                    }
                  }
                }
              }
            ]
          }
        }
      },
      "templates": {
        "welcome": {
          "background": "#ffffff"
        },
        "achievement": {
          "background": "#ffffff"
        },
        "memeCollected": {
          "background": "#ffffff"
        },
        "body": {
          "background": "#ffffff"
        }
      }
    }
  }
}
